(%ex "1.19")

; exercise 1.19, page 47

(define (fib n)
  (fib-iter 1 0 0 1 n))
(define (fib-iter a b p q count)
  (cond ((= count 0) b)
        ((even? count)
         (fib-iter a
                   b
                   (+ (square q) (square p))	; compute p'
                   (+ (* 2 p q) (square q))  	; compute q'
                   (/ count 2)))
        (else (fib-iter (+ (* b q) (* a q) (* a p))
                        (+ (* b p) (* a q))
                        p
                        q
                        (- count 1)))))

(fib 0)
(fib 3)
(fib 4)
(fib 6)
(fib 12)
(fib 73)
